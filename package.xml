      <email>ulf.wendel@phpdoc.de</email>
    </maintainer>
  </maintainers>
  <license>PHP License</license>
  <release>
    <version>1.5.x</version>
    <date>2002-11-08</date>
    <state>stable</state>
    <notes>
	- Added Lorenzo Alberton's MDB_Container
    </notes>
    <filelist>
      <dir name="/" baseinstalldir="Cache">
        <file role="php" baseinstalldir="/" name="Cache.php" />
        <file role="php" name="Application.php" />
        <file role="php" name="Container.php" />
        <file role="php" name="DB.php" />
        <file role="php" name="Error.php" />
        <file role="php" name="Function.php" />
        <file role="php" name="Graphics.php" />
        <file role="php" name="HTTP_Request.php" />
        <file role="php" name="Output.php" />
        <file role="php" name="OutputCompression.php" />
        <dir name="Container">
          <file role="php" name="db.php" />
          <file role="php" name="dbx.php" />
          <file role="php" name="mdb.php" />
          <file role="data" name="mdb_cache_schema.xml" />
          <file role="php" name="file.php" />
          <file role="php" name="msession.php" />
          <file role="php" name="phplib.php" />
          <file role="php" name="shm.php" />
        </dir>
      </dir>
    </filelist>
    <deps>
      <dep type="pkg" rel="has">HTTP_Request</dep>
    </deps>
  </release>
