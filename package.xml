  <release>
    <version>1.4</version>
    <date>2002-05-24</date>
    <state>stable</state>
    <notes>
Fixed packaging.
    </notes>
    <filelist>
      <dir name="/">
        <dir name="Container" baseinstalldir="Cache">
          <file role="php" name="db.php" />
          <file role="php" name="dbx.php" />
          <file role="php" name="file.php" />
          <file role="php" name="msession.php" />
          <file role="php" name="phplib.php" />
          <file role="php" name="shm.php" />
        </dir>
        <file role="php" name="Cache.php" />
        <file baseinstalldir="Cache" role="php" name="Application.php" />
        <file baseinstalldir="Cache" role="php" name="Container.php" />
        <file baseinstalldir="Cache" role="php" name="DB.php" />
        <file baseinstalldir="Cache" role="php" name="Error.php" />
        <file baseinstalldir="Cache" role="php" name="Function.php" />
        <file baseinstalldir="Cache" role="php" name="Graphics.php" />
        <file baseinstalldir="Cache" role="php" name="Output.php" />
        <file baseinstalldir="Cache" role="php" name="OutputCompression.php" />
      </dir>
    </filelist>
  </release>
